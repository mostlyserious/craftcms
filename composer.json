{
    "name": "mostlyserious/craftcms",
    "description": "A Craft CMS Starter",
    "type": "project",
    "license": "Unlicense",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "craftcms/aws-s3": "2.2.3",
        "craftcms/ckeditor": "4.11.0",
        "craftcms/cms": "5.8.21",
        "craftpulse/craft-colour-swatches": "5.1.0",
        "diginov/craft-sentry-logger": "5.0.2",
        "mostlyserious/craft-markerio": "2.2.0",
        "npm-asset/fortawesome--fontawesome-pro": "7.0.0",
        "nystudio107/craft-retour": "5.0.12",
        "nystudio107/craft-seomatic": "5.1.19",
        "nystudio107/craft-vite": "5.0.1",
        "putyourlightson/craft-sendgrid": "3.0.0",
        "putyourlightson/craft-sherlock": "5.2.0",
        "roberskine/craft-user-manual": "5.0.4",
        "spicyweb/craft-embedded-assets": "5.4.4",
        "symfony/http-foundation": "^7.3.7",
        "topshelfcraft/environment-label": "5.0.0",
        "verbb/expanded-singles": "3.0.3",
        "verbb/icon-picker": "3.0.6",
        "vlucas/phpdotenv": "^5.4.0",
        "webrgp/craft-ignition": "v1.0.7"
    },
    "require-dev": {
        "craftcms/generator": "^2.0.0",
        "laravel/pint": "^1.25",
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/"
        }
    },
    "config": {
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.3"
        }
    },
    "scripts": {
        "pint": "./vendor/bin/pint",
        "post-create-project-cmd": [
            "./utility/install.sh"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.craftcms.com",
            "canonical": false
        },
        {
            "type": "package",
            "package": {
                "name": "npm-asset/fortawesome--fontawesome-pro",
                "version": "7.0.0",
                "type": "npm-asset",
                "dist": {
                    "type": "tar",
                    "url": "https://npm.fontawesome.com/@fortawesome/fontawesome-pro/-/7.0.0/fontawesome-pro-7.0.0.tgz"
                }
            }
        }
    ]
}
